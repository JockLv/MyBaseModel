<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.mybasemodel">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".base.MyApp">

        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5ac9b9788f4a9d61e600000f" />
        <!-- Channel ID用来标识App的推广渠道，作为推送消息时给用户分组的一个维度。-->
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="my_BaseModel" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity" />
        <activity android:name=".NotifyActivity" />
        <activity android:name=".AOActivity" />
        <activity android:name=".anim.AnimationOneActivity"
            android:theme="@style/SecondActivityTheme"/>
        <activity android:name=".AODefaultActivity" />
        <activity android:name=".anim.AnimationTwoActivity"
            android:theme="@style/SecondActivityTheme"/>
        <activity android:name=".anim.AnimationThreeActivity" />
        <activity android:name=".CarControlActivity" />

    </application>

</manifest>